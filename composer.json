{
    "name": "winnerpicker/instagram-sdk",
    "description": "Instagram SDK",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": "5.3.*",
        "guzzlehttp/guzzle": "^6.2",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "Winnerpicker\\Instagram\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
